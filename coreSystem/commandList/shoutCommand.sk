command /shout [<text>]:
    permission: skript.admin
    permission message: Unknown command. Type "/help" for help.
    trigger:
        if arg-1 is set:
            broadcast formatted "<tooltip:&7Shouted by %player% %now formatted as "M/dd/yy"%.>&3[SHOUT] » &f%arg-1%"
        else:
            send "&3[SHOUT] » &cPlease specify a message."